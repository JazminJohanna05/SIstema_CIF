<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICategoriaService" />
                <binding name="BasicHttpBinding_IUsuarioService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:55544/Services/CategoriaService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICategoriaService"
                contract="Proxies.Categoria.ICategoriaService" name="BasicHttpBinding_ICategoriaService" />
            <endpoint address="http://localhost:55544/Services/ModuloComunesCIF/UsuarioService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUsuarioService"
                contract="Proxies.Usuario.IUsuarioService" name="BasicHttpBinding_IUsuarioService" />
        </client>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="Sistema_CIF.Services.ClienteService">
                <endpoint address="" binding="basicHttpBinding" contract="Sistema_CIF.Services.IClienteService">
                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/Sistema_CIF.Services/ClienteService/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>